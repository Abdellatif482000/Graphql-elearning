{
  "name": "achievementService",
  "packageManager": "yarn@4.5.2",
  "type": "module",
  "scripts": {
    "start": "NODE_ENV=prod node dist/server.js",
    "build:tsc": "tsc",
    "build:swc:watch": "swc src -d dist --watch --copy-files",
    "type-check:watch": "tsc --noEmit --watch",
    "start:watch": "NODE_ENV=dev yarn nodemon dist/src/server.js",
    "dev": "concurrently  --prefix-colors 'bgBlue.bold,bgYellow.bold,bgGreen.bold' --names 'types,build,watch' 'yarn type-check:watch' 'yarn build:swc:watch'  'yarn start:watch'"
  },
  "devDependencies": {
    "@apollo/rover": "^0.27.0",
    "@swc-node/core": "^1.13.3",
    "@swc-node/register": "^1.10.9",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.11.1",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "@types/pg": "^8.11.11",
    "nodemon": "^3.1.9",
    "prisma": "^6.3.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@apollo/gateway": "^2.9.3",
    "@apollo/server": "^4.11.3",
    "@apollo/subgraph": "^2.9.3",
    "@graphql-tools/graphql-file-loader": "^8.0.14",
    "@graphql-tools/load": "^8.0.14",
    "@graphql-tools/merge": "^9.0.19",
    "@graphql-tools/schema": "^10.0.18",
    "@prisma/client": "^6.3.1",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.3",
    "chokidar": "^4.0.3",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "glob": "^11.0.1",
    "graphql": "^16.10.0",
    "graphql-http": "^1.22.4",
    "graphql-tag": "^2.12.6",
    "graphql-voyager": "^2.1.0",
    "inversify": "^6.2.1",
    "pg": "^8.13.3",
    "reflect-metadata": "^0.2.2"
  }
}
